<?xml version="1.0" encoding="ISO-8859-1"?>


<alg_conf>

  <param_set name="Velocity0">
    <param type="string" name="CommonParam"> BoostedDarkMatter,WeakInt,NonResBackground,CKM,FermiGas  </param>
    <!-- Defalut? -->
    <param type="alg" name="XSec-Integrator"> genie::ReinSehgalDMRESPXSec/NoPauliBlock         </param>
    <param type="int" name="velocity-mode">   0                               </param>

    <param type="double" name="RES-DM-XSecScale"> 1.000 </param>

    <!--
	 ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
	 Constants in Feynman-Kislinger-Ravndal model for baryon excitation used in the Rein-Sehgal
	 resonance model
    -->
    <param type="double" name="RES-Omega"> 1.05  </param>
    <param type="double" name="RES-Zeta">  0.76338 </param> <!-- 0.75 -->


    <!--
	 ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
	 Axial and vector masses for resonance production
    -->
    <param type="double" name="RES-Ma"> 1.120 </param>
    <param type="double" name="RES-Mv"> 0.840 </param>

  </param_set>

  <param_set name="Velocity2">
    <param type="string" name="CommonParam"> BoostedDarkMatter,WeakInt,NonResBackground,CKM,FermiGas  </param>
    <param type="alg" name="XSec-Integrator"> genie::ReinSehgalDMRESPXSec/NoPauliBlock         </param>
    <param type="int" name="velocity-mode">   2                               </param>

    <param type="double" name="RES-DM-XSecScale"> 1.000 </param>

    <!--
	 ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
	 Constants in Feynman-Kislinger-Ravndal model for baryon excitation used in the Rein-Sehgal
	 resonance model
    -->
    <param type="double" name="RES-Omega"> 1.05  </param>
    <param type="double" name="RES-Zeta">  0.76338 </param> <!-- 0.75 -->


    <!--
	 ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
	 Axial and vector masses for resonance production
    -->
    <param type="double" name="RES-Ma"> 1.120 </param>
    <param type="double" name="RES-Mv"> 0.840 </param>

  </param_set>

</alg_conf>
